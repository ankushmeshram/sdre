package de.dfki.isreal.data;

import java.util.List;

/**
 * Interface that represents a parameter of a service (input, output, local)
 * Every parameter has a name (his uri) and a list of types (uris to classes)
 * 
 * @author stenes
 *
 */
public interface Parameter {
	
	/**
	 * Returns the Uri String of the name of the variable.
	 * @return
	 */
	public String getName();
	
	/**
	 * Returns the list of types (as Uri String) of the parameter.
	 * @return
	 */
	public List<String> getTypes();

}
